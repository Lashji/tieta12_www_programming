{
    "printWidth": 80,
    "trailingComma": "yes",
    "tabWidth": 4,
    "semi": true,
    "singleQuote": true,
    "useTabs": true,
    "quoteProps": "as-needed",
    "bracketSpacing": true,
    "arrowParens": "always",
    "endOfLine": "lf",
    "overrides": [
        {
            "files": ".prettierrc",
            "options": {
                "parser": "json"
            }
        }
    ]
}
